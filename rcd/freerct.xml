	<file target="people_animation.rcd" magic="RCDF" version="1">
		<!-- Person type recolouring. -->
		<gameblock magic="PRSG" version="1">
			<field name="recolourings">
				<struct name="1">
					<field name="person-type" value="pillar"/>
					<field name="recolouring1">
						<bitfield name="orig"><field value="grey"/></bitfield>
						<bitfield name="replace"><field value="yellow"/><field value="green"/><field value="blue"/>
								<field value="orange"/><field value="pink"/></bitfield>
					</field>
					<field name="recolouring2">
						<bitfield name="orig"><field value="invalid"/></bitfield>
						<bitfield name="replace"/>
					</field>
					<field name="recolouring3">
						<bitfield name="orig"><field value="invalid"/></bitfield>
						<bitfield name="replace"/>
					</field>
				</struct>
			</field>
		</gameblock>

		<!-- NE walking -->
		<gameblock magic="ANIM" version="2">
			<field name="person_type" value="pillar"/>
			<field name="anim_type"   value="walk_ne"/>
			<field name="frames">
				<struct name="1">
					<field name="duration" value="40"/>
					<field name="change-x" value="-4"/>
					<field name="change-y" value="0"/>
				</struct>
			</field>
		</gameblock>
		<gameblock magic="ANSP" version="1">
			<field name="width"       value="64"/>  <!-- Zoom-width -->
			<field name="person_type" value="pillar"/>
			<field name="anim_type"   value="walk_ne"/>
			<field name="frames">
				<image x-base="0" y-base="0"  width="10" height="27"
				       x-offset="-5" y-offset="-23" fname="../sprites/pillar.png"/>
			</field>
		</gameblock>

		<!-- SE walking -->
