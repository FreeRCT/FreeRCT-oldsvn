	<file target="buildarrows_8bpp64.rcd" magic="RCDF" version="1">
		<gameblock magic="BDIR" version="1">
			<field name="width" value="64" />  <!-- Zoom-width -->
			<sheet x-base="0" y-base="0" x-step="64" y-step="64"
			       fname="../sprites_src/objects/1x1/gui/orthbuildmark8bpp64.png"
			       names="ne,se,sw,nw"
			       x-offset="-32" y-offset="-33" width="64" height="64" />
		</gameblock>

		<!-- Gui slope sprites -->
		<gameblock magic="GSLP" version="1">
			<sheet x-base="0" y-base="0" x-step="32" y-step="32"
			       fname="../sprites_src/gui/contruction/slopes_8bpp_32px.png"
			       names="vert-down, steep-down, gentle-down, level, gentle-up, steep-up, vert-up"
			       x-offset="0" y-offset="0" width="32" height="32" />
		</gameblock>

		<gameblock magic="GROT" version="1">
			<image name="3d-positive" x-base="192" y-base="224"
