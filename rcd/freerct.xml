	<file target="borderhighlight_8bpp64.rcd" magic="RCDF" version="1">
		<gameblock magic="TSEL" version="1">
			<field name="tile_width" value="64"/>
			<field name="z_height"   value="16"/>
			<sheet x-base="0" y-base="0" x-step="64" y-step="64"
