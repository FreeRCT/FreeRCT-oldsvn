<structures magic="RCDF" version="1">
	<enum name="PersonType" type="uint8">
		<value name="any"    value="0">Any person type is fine (eg no visible persons in the animation)</value>
		<value name="pillar" value="8">Guests from the Planet of Pillar (test graphics)</value>
		<value name="earth"  value="16">A more common kind of people</value>
	</enum>

	<enum name="AnimationType" type="uint16">
		<value name="walk_ne" value="1">Walk in north-east direction</value>
		<value name="walk_se" value="2">Walk in south-east direction</value>
		<value name="walk_sw" value="3">Walk in south-west direction</value>
		<value name="walk_nw" value="4">Walk in north-west direction</value>
	</enum>

	<struct name="FrameDataStruct">
		<field name="duration"><type name="uint16"/></field>
		<field name="change-x"><type name="int16"/></field>
		<field name="change-y"><type name="int16"/></field>
	</struct>

	<block magic="ANIM" minversion="1" maxversion="2">
		<field name="width" minversion="1" maxversion="1"><type name="uint16"/><comment>Zoom-width of a tile of the surface</comment></field>
		<field name="person_type">                        <type name="PersonType"/><comment>Type of persons in the animation</comment></field>
		<field name="anim_type">                          <type name="AnimationType"/><comment>Animation being defined</comment></field>
		<field name="frames"><type name="list" count="uint16"><type name="FrameDataStruct"/></type>
				<comment>Definition of the frames</comment></field>
	</block>

	<block magic="ANSP" minversion="1" maxversion="1">
		<field name="width">      <type name="uint16"/><comment>Zoom-width of a tile</comment></field>
		<field name="person_type"><type name="PersonType"/><comment>Type of persons in the animation</comment></field>
		<field name="anim_type">  <type name="AnimationType"/><comment>Animation being defined</comment></field>
		<field name="frames">
			<type name="list" count="uint16"><type name="sprite"/></type>
			<comment>Sprites of the animation for a single tile width</comment></field>
	</block>

	<block magic="BDIR" minversion="1" maxversion="1">
		<field name="width"><type name="uint16"/><comment>Zoom-width of a tile of the surface</comment></field>
		<field name="ne">   <type name="sprite"/><comment>North-east direction</comment></field>
		<field name="se">   <type name="sprite"/><comment>South-east direction</comment></field>
		<field name="sw">   <type name="sprite"/><comment>South-west direction</comment></field>
		<field name="nw">   <type name="sprite"/><comment>North-west direction</comment></field>
	</block>

	<!-- Foundations -->
	<enum name="FoundationType" type="uint16">
		<value name="reserved" value="0">Empty, do not use</value>
		<value name="ground"   value="16">Ground</value>
		<value name="wood"     value="32">Wood</value>
		<value name="brick"    value="48">Stone bricks</value>
	</enum>

	<block magic="FUND" minversion="1" maxversion="1">
		<field name="found_type"> <type name="FoundationType"/><comment>Type of foundation</comment></field>
		<field name="tile_width"> <type name="uint16"/><comment>Zoom-width of a tile</comment></field>
		<field name="tile_height"><type name="uint16"/><comment>Change in Z height of the tiles</comment></field>
		<field name="se_0e"><type name="sprite"/><comment>Vertical south-east foundation, east up, south down</comment></field>
		<field name="se_s0"><type name="sprite"/><comment>Vertical south-east foundation, east down, south up</comment></field>
		<field name="se_se"><type name="sprite"/><comment>Vertical south-east foundation, east up, south up</comment></field>
		<field name="sw_s0"><type name="sprite"/><comment>Vertical south-west foundation, south up, west down</comment></field>
		<field name="sw_0w"><type name="sprite"/><comment>Vertical south-west foundation, south down, west up</comment></field>
		<field name="sw_sw"><type name="sprite"/><comment>Vertical south-west foundation, south up, west up</comment></field>
	</block>

	<!-- Widget border sprites -->
	<enum name="BorderWidgetType" type="uint16">
		<value name="reserved"               value="0">Invalid, do not use</value>
		<value name="window_border"          value="16">Window border</value>
		<value name="titlebar"               value="32">Titlebar</value>
		<value name="button"                 value="48">button</value>
		<value name="pressed_button"         value="49">pressed button</value>
		<value name="rounded_button"         value="52">rounded button</value>
		<value name="pressed_rounded_button" value="53">pressed rounded button</value>
		<value name="frame"                  value="64">frame</value>
		<value name="panel"                  value="68">panel</value>
		<value name="inset"                  value="80">inset frame</value>
	</enum>

	<block magic="GBOR" minversion="1" maxversion="1">
		<field name="widget_num">         <type name="BorderWidgetType"/></field>
		<field name="border-width-top">   <type name="uint8"/></field>
		<field name="border-width-left">  <type name="uint8"/></field>
		<field name="border-width-right"> <type name="uint8"/></field>
		<field name="border-width-bottom"><type name="uint8"/></field>
		<field name="min-width-rect">     <type name="uint8"/></field>
		<field name="min-height-rect">    <type name="uint8"/></field>
		<field name="hor-stepsize">       <type name="uint8"/></field>
		<field name="vert-stepsize">      <type name="uint8"/></field>
		<field name="top-left">           <type name="sprite"/></field>
		<field name="top-middle">         <type name="sprite"/></field>
		<field name="top-right">          <type name="sprite"/></field>
		<field name="left">               <type name="sprite"/></field>
		<field name="middle">             <type name="sprite"/></field>
		<field name="right">              <type name="sprite"/></field>
		<field name="bottom-left">        <type name="sprite"/></field>
		<field name="bottom-middle">      <type name="sprite"/></field>
		<field name="bottom-right">       <type name="sprite"/></field>
	</block>

	<!-- Check widgets -->
	<enum name="CheckWidgetType" type="uint16">
		<value name="reserved"     value="0">Invalid, do not use</value>
		<value name="check_box"    value="96">Check box</value>
		<value name="radio_button" value="112">Radio button</value>
	</enum>

	<block magic="GCHK" minversion="1" maxversion="1">
		<field name="widget_num">    <type name="CheckWidgetType"/></field>
		<field name="empty" >        <type name="sprite"/></field>
		<field name="filled" >       <type name="sprite"/></field>
		<field name="pressed" >      <type name="sprite"/></field>
		<field name="pressed-filled"><type name="sprite"/></field>
		<field name="shaded" >       <type name="sprite"/></field>
		<field name="shaded-filled"> <type name="sprite"/></field>
	</block>

	<enum name="ScrollbarWidgetType" type="uint16">
		<value name="hor"         value="160">Horizontal scroll bar + button</value>
		<value name="shaded_hor"  value="161">Shaded horizontal scroll bar + button</value>
		<value name="vert"        value="176">Vertical scroll bar + button</value>
		<value name="shaded_vert" value="177">Shaded vertical scroll bar + button</value>
	</enum>

	<block magic="GSCL" minversion="1" maxversion="1">
		<field name="minimal-length-scrollbar"><type name="uint8"/></field>
		<field name="stepsize-scrollbar">      <type name="uint8"/></field>
		<field name="minimal-length-select">   <type name="uint8"/></field>
		<field name="stepsize-select">         <type name="uint8"/></field>
		<field name="widget_num">              <type name="ScrollbarWidgetType"/></field>
		<field name="leftup">                  <type name="sprite"/></field>
		<field name="rightdown">               <type name="sprite"/></field>
		<field name="leftup-pressed">          <type name="sprite"/></field>
		<field name="rightdown-pressed">       <type name="sprite"/></field>
		<field name="leftup-under">            <type name="sprite"/></field>
		<field name="middle-under">            <type name="sprite"/></field>
		<field name="rightdown-under">         <type name="sprite"/></field>
		<field name="leftup-select">           <type name="sprite"/></field>
		<field name="middle-select">           <type name="sprite"/></field>
		<field name="rightdown-select">        <type name="sprite"/></field>
		<field name="leftup-pressed-select">   <type name="sprite"/></field>
		<field name="middle-pressed-select">   <type name="sprite"/></field>
		<field name="rightdown-pressed-select"><type name="sprite"/></field>
	</block>

	<enum name="SliderWidgetType" type="uint16">
		<value name="hor"         value="128">Horizontal slider bar + button</value>
		<value name="shaded_hor"  value="129">Shaded horizontal slider bar + button</value>
		<value name="vert"        value="144">Vertical slider bar + button</value>
		<value name="shaded_vert" value="145">Shaded vertical slider bar + button</value>
	</enum>

	<block magic="GSLI" minversion="1" maxversion="2">
		<field name="min-length" minversion="1" maxversion="1"><type name="uint8"/><comment>Minimal length of the bar</comment></field>
		<field name="min-length" minversion="2"               ><type name="uint16"/><comment>Minimal length of the bar</comment></field>

		<field name="stepsize"   minversion="1" maxversion="1"><type name="uint8"/><comment>Stepsize of the bar</comment></field>
		<field name="stepsize"   minversion="2"               ><type name="uint16"/><comment>Stepsize of the bar</comment></field>

		<field name="width"      minversion="1" maxversion="1"><type name="uint8"/><comment>Width of the slider button</comment></field>
		<field name="width"      minversion="2"               ><type name="uint16"/><comment>Width of the slider button</comment></field>

		<field name="widgettype"><type name="SliderWidgetType"/><comment>Widget type</comment></field>
		<field name="left">      <type name="sprite"/></field>
	</block>

	<block magic="GSLP" minversion="1" maxversion="4">
		<field name="vert-down">  <type name="sprite"/><comment>Slope vertically down</comment></field>
		<field name="steep-down"> <type name="sprite"/><comment>Slope steeply down</comment></field>
		<field name="gentle-down"><type name="sprite"/><comment>Slope gently down</comment></field>
		<field name="level">      <type name="sprite"/><comment>Level slope</comment></field>
		<field name="gentle-up">  <type name="sprite"/><comment>Slope gently up</comment></field>
		<field name="steep-up">   <type name="sprite"/><comment>Slope steeply up</comment></field>
		<field name="vert-up">    <type name="sprite"/><comment>Slope vertically up</comment></field>

		<field name="2d-positive" minversion="2"><type name="sprite"/><comment>Flat rotation, positive direction</comment></field>
		<field name="2d-negative" minversion="2"><type name="sprite"/><comment>Flat rotation, negative direction</comment></field>
		<field name="3d-positive" minversion="2"><type name="sprite"/><comment>Diametric rotation, positive direction</comment></field>
		<field name="3d-negative" minversion="2"><type name="sprite"/><comment>Diametric rotation, negative direction</comment></field>

		<field name="close-sprite"    minversion="3"><type name="sprite"/></field>
		<field name="maximise-sprite" minversion="3" maxversion="3"><type name="sprite"/></field>
		<field name="minimise-sprite" minversion="3" maxversion="3"><type name="sprite"/></field>
		<field name="terraform-dot"   minversion="4"><type name="sprite"/></field>

		<field name="texts" minversion="2"><type name="text" file="gui_strings.txt"/></field>
	</block>

	<block magic="PARE" minversion="1" maxversion="1">
		<field name="tile_width"> <type name="uint16"/></field>
		<field name="z_height">   <type name="uint16"/></field>
		<!-- sprites viewed to the north. -->
		<field name="n#">   <type name="sprite"/></field>
		<field name="n#n">  <type name="sprite"/></field>
		<field name="n#e">  <type name="sprite"/></field>
		<field name="n#ne"> <type name="sprite"/></field>
		<field name="n#s">  <type name="sprite"/></field>
		<field name="n#ns"> <type name="sprite"/></field>
		<field name="n#es"> <type name="sprite"/></field>
		<field name="n#nes"><type name="sprite"/></field>
		<field name="n#w">  <type name="sprite"/></field>
		<field name="n#nw"> <type name="sprite"/></field>
		<field name="n#ew"> <type name="sprite"/></field>
		<field name="n#new"><type name="sprite"/></field>
		<field name="n#sw"> <type name="sprite"/></field>
		<field name="n#nsw"><type name="sprite"/></field>
		<field name="n#esw"><type name="sprite"/></field>
		<field name="n#N">  <type name="sprite"/></field>
		<field name="n#E">  <type name="sprite"/></field>
		<field name="n#S">  <type name="sprite"/></field>
		<field name="n#W">  <type name="sprite"/></field>
	</block>

	<!-- Path sprites -->
	<enum name="PathType" type="uint16">
		<value name="reserved" value="0">Empty, do not use</value>
		<value name="concrete" value="16">Concrete paths</value>
	</enum>

	<block magic="PATH" minversion="1" maxversion="1">
		<field name="path_type"> <type name="PathType"/></field>
		<field name="tile_width"><type name="uint16"/></field>
		<field name="z_height">  <type name="uint16"/></field>
		<!-- sprites for the path. -->
		<field name="empty">              <type name="sprite"/></field>
		<field name="ne">                 <type name="sprite"/></field>
		<field name="se">                 <type name="sprite"/></field>
		<field name="ne_se">              <type name="sprite"/></field>
		<field name="ne_se_e">            <type name="sprite"/></field>
		<field name="sw">                 <type name="sprite"/></field>
		<field name="ne_sw">              <type name="sprite"/></field>
		<field name="se_sw">              <type name="sprite"/></field>
		<field name="se_sw_s">            <type name="sprite"/></field>
		<field name="ne_se_sw">           <type name="sprite"/></field>
		<field name="ne_se_sw_e">         <type name="sprite"/></field>
		<field name="ne_se_sw_s">         <type name="sprite"/></field>
		<field name="ne_se_sw_e_s">       <type name="sprite"/></field>
		<field name="nw">                 <type name="sprite"/></field>
		<field name="ne_nw">              <type name="sprite"/></field>
		<field name="ne_nw_n">            <type name="sprite"/></field>
		<field name="nw_se">              <type name="sprite"/></field>
		<field name="ne_nw_se">           <type name="sprite"/></field>
		<field name="ne_nw_se_n">         <type name="sprite"/></field>
		<field name="ne_nw_se_e">         <type name="sprite"/></field>
		<field name="ne_nw_se_n_e">       <type name="sprite"/></field>
		<field name="nw_sw">              <type name="sprite"/></field>
		<field name="nw_sw_w">            <type name="sprite"/></field>
		<field name="ne_nw_sw">           <type name="sprite"/></field>
		<field name="ne_nw_sw_n">         <type name="sprite"/></field>
		<field name="ne_nw_sw_w">         <type name="sprite"/></field>
		<field name="ne_nw_sw_n_w">       <type name="sprite"/></field>
		<field name="nw_se_sw">           <type name="sprite"/></field>
		<field name="nw_se_sw_s">         <type name="sprite"/></field>
		<field name="nw_se_sw_w">         <type name="sprite"/></field>
		<field name="nw_se_sw_s_w">       <type name="sprite"/></field>
		<field name="ne_nw_se_sw">        <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n">      <type name="sprite"/></field>
		<field name="ne_nw_se_sw_e">      <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_e">    <type name="sprite"/></field>
		<field name="ne_nw_se_sw_s">      <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_s">    <type name="sprite"/></field>
		<field name="ne_nw_se_sw_e_s">    <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_e_s">  <type name="sprite"/></field>
		<field name="ne_nw_se_sw_w">      <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_w">    <type name="sprite"/></field>
		<field name="ne_nw_se_sw_e_w">    <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_e_w">  <type name="sprite"/></field>
		<field name="ne_nw_se_sw_s_w">    <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_s_w">  <type name="sprite"/></field>
		<field name="ne_nw_se_sw_e_s_w">  <type name="sprite"/></field>
		<field name="ne_nw_se_sw_n_e_s_w"><type name="sprite"/></field>
		<field name="ramp_ne">            <type name="sprite"/></field>
		<field name="ramp_nw">            <type name="sprite"/></field>
		<field name="ramp_se">            <type name="sprite"/></field>
		<field name="ramp_sw">            <type name="sprite"/></field>
	</block>

	<!-- Person type graphics. -->
	<enum name="ColourRange" type="uint8">
		<value name="grey"        value= "0"/>
		<value name="green-brown" value= "1"/>
		<value name="brown"       value= "2"/>
		<value name="yellow"      value= "3"/>
		<value name="dark-red"    value= "4"/>
		<value name="dark-green"  value= "5"/>
		<value name="light-green" value= "6"/>
		<value name="green"       value= "7"/>
		<value name="light-red"   value= "8"/>
		<value name="dark-blue"   value= "9"/>
		<value name="blue"        value="10"/>
		<value name="light-blue"  value="11"/>
		<value name="purple"      value="12"/>
		<value name="red"         value="13"/>
		<value name="orange"      value="14"/>
		<value name="sea-green"   value="15"/>
		<value name="pink"        value="16"/>
		<value name="beige"       value="17"/>
		<value name="invalid"     value="18"/>
	</enum>

	<struct name="PersonGraphicsStruct">
		<field name="person-type"><type name="PersonType"/></field>
		<field name="recolouring1"><type name="bitset" storage="uint32">
			<bitfield name="orig"    enum="ColourRange" min-count="1" max-count="1" start-bit="24"/>
			<bitfield name="replace" enum="ColourRange" min-count="0" max-count="*" start-bit="0" bit-numbers=""/></type>
		</field>
		<field name="recolouring2"><type name="bitset" storage="uint32">
			<bitfield name="orig"    enum="ColourRange" min-count="1" max-count="1" start-bit="24"/>
			<bitfield name="replace" enum="ColourRange" min-count="0" max-count="*" start-bit="0" bit-numbers=""/></type>
		</field>
		<field name="recolouring3"><type name="bitset" storage="uint32">
			<bitfield name="orig"    enum="ColourRange" min-count="1" max-count="1" start-bit="24"/>
			<bitfield name="replace" enum="ColourRange" min-count="0" max-count="*" start-bit="0" bit-numbers=""/></type>
		</field>
	</struct>

	<block magic="PRSG" minversion="1" maxversion="1">
		<field name="recolourings"><type name="list" count="uint8"><type name="PersonGraphicsStruct"/></type></field>
	</block>

	<!-- Platforms (flat areas to support a path in the air) -->
	<enum name="PlatformType" type="uint16">
		<value name="reserved" value="0">Empty, do not use</value>
		<value name="wood"     value="16">Wooden platforms</value>
	</enum>

	<block magic="PLAT" minversion="1" maxversion="2">
		<field name="width">  <type name="uint16"/><comment>Zoom-width of a tile of the surface</comment></field>
		<field name="height"> <type name="uint16"/><comment>Change in Z height of the tiles</comment></field>
		<field name="type">   <type name="PlatformType"/><comment>Platform type</comment></field>

		<field name="ns">     <type name="sprite"/><comment>Flat platform for north and south view</comment></field>
		<field name="ew">     <type name="sprite"/><comment>Flat platform for east and west view</comment></field>

		<field name="ramp-ne"><type name="sprite"/><comment>Platform is raised at the NE edge</comment></field>
		<field name="ramp-se"><type name="sprite"/><comment>Platform is raised at the SE edge</comment></field>
		<field name="ramp-sw"><type name="sprite"/><comment>Platform is raised at the SW edge</comment></field>
		<field name="ramp-nw"><type name="sprite"/><comment>Platform is raised at the NW edge</comment></field>

		<field name="right-ramp-ne" minversion="2"><type name="sprite"/>
				<comment>Platform with right leg is raised at the NE edge</comment></field>
		<field name="right-ramp-se" minversion="2"><type name="sprite"/>
				<comment>Platform with right leg is raised at the SE edge</comment></field>
		<field name="right-ramp-sw" minversion="2"><type name="sprite"/>
				<comment>Platform with right leg is raised at the SW edge</comment></field>
		<field name="right-ramp-nw" minversion="2"><type name="sprite"/>
				<comment>Platform with right leg is raised at the NW edge</comment></field>

		<field name="left-ramp-ne" minversion="2"><type name="sprite"/>
				<comment>Platform with left leg is raised at the NE edge</comment></field>
		<field name="left-ramp-se" minversion="2"><type name="sprite"/>
				<comment>Platform with left leg is raised at the SE edge</comment></field>
		<field name="left-ramp-sw" minversion="2"><type name="sprite"/>
				<comment>Platform with left leg is raised at the SW edge</comment></field>
		<field name="left-ramp-nw" minversion="2"><type name="sprite"/>
				<comment>Platform with left leg is raised at the NW edge</comment></field>
	</block>

	<enum name="Entrance" type="uint8">
		<value name="ne" value="0"/>
		<value name="se" value="1"/>
		<value name="sw" value="2"/>
		<value name="nw" value="3"/>
	</enum>
	<enum name="ItemType" type="uint8">
		<value name="nothing"       value="0"/>
		<value name="drink"         value="8"/>
		<value name="icecream"      value="9"/>
		<value name="non-salt-food" value="16"/>
		<value name="salt-food"     value="24"/>
		<value name="umbrella"      value="32"/>
		<value name="map"           value="40"/>
	</enum>

	<block magic="SHOP" minversion="1" maxversion="4">
		<field name="width"> <type name="uint16"/><comment>Zoom-width of a tile of the surface</comment></field>
		<field name="height" minversion="1" maxversion="3"><type name="uint16"/><comment>Height of the shop in voxels</comment></field>
		<field name="height" minversion="4"><type name="uint8"/><comment>Height of the shop in voxels</comment></field>
		<field name="flags" minversion="4"><type name="bitset" storage="uint8">
			<bitfield name="entrances" enum="Entrance" min-count="1" max-count="4" start-bit="0" bit-numbers=""/></type></field>
		<field name="ne">    <type name="sprite"/><comment>North-east direction</comment></field>
		<field name="se">    <type name="sprite"/><comment>South-east direction</comment></field>
		<field name="sw">    <type name="sprite"/><comment>South-west direction</comment></field>
		<field name="nw">    <type name="sprite"/><comment>North-west direction</comment></field>
